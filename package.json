{
  "name": "react-redux-ts-webpack-jest-prettier-boilerplate",
  "version": "1.0.1",
  "private": true,
  "description": "A frontend boilerplate with React, Redux, Typescript, webpack, jest",
  "keywords": [
    "react",
    "redux",
    "typescript",
    "jest",
    "prettier",
    "webpack",
    "boilerplate"
  ],
  "main": "index.js",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "jest --watchAll",
    "start:dev": "webpack-dev-server --mode development --hot --progress --color --port 3000 --open --watch",
    "build": "webpack -p --progress --colors",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,css}\""
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@types/classnames": "^2.2.7",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.6",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^7.0.0",
    "@types/react-router-dom": "^4.3.2",
    "@types/redux": "^3.6.0",
    "@types/redux-actions": "2.3.1",
    "@types/webpack": "^4.4.23",
    "@types/webpack-env": "1.13.6",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "html-loader": "^1.0.0-alpha.0",
    "html-webpack-plugin": "^4.0.0-alpha",
    "jest": "^24.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss": "^7.0.13",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "postcss-reporter": "^6.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.15.3",
    "react-hot-loader": "^4.6.3",
    "redux-devtools-extension": "^2.13.7",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.4",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "normalize.css": "^8.0.1",
    "npm": "^6.9.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.(js|ts|tsx|jsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^components/(.*)": "<rootDir>/src/app/components/$1"
    },
    "setupFiles": [
      "./src/setupTests.ts"
    ],
    "testURL": "http://localhost:1234/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pantelisGeo90/react-redux-ts-webpack-jest-prettier-boilerplate.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/pantelisGeo90/react-redux-ts-webpack-jest-prettier-boilerplate/issues"
  },
  "homepage": "https://github.com/pantelisGeo90/react-redux-ts-webpack-jest-prettier-boilerplate#readme"
}
